[Unit]
Description=ARL Worker Service
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/root/ARL_2.6.2/ARL
ExecStart=/root/miniconda3/envs/arl/bin/celery -A app.celerytask.celery worker -l info -Q arltask -n arltask -c 2 -O fair -f arl_worker.log

[Install]
WantedBy=multi-user.target

